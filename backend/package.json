{
  "name": "revenue-intelligence-backend",
  "version": "1.0.0",
  "description": "Backend API for Revenue Intelligence CRM Overlay",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.nodemon.json src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "inspect-fields": "ts-node --project tsconfig.nodemon.json src/scripts/inspect-account-fields.ts",
    "inspect-all-fields": "ts-node --project tsconfig.nodemon.json src/scripts/inspect-all-fields.ts"
  },
  "keywords": [
    "salesforce",
    "revenue-intelligence",
    "crm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "jsforce": "^2.0.0-beta.29",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "@types/express-session": "^1.17.10",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  }
}
